{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "rootDir": "./src",
    "typeRoots": [
      "./src/@types",
      "./custom_typings",
      "./@types",
      "./node_modules/@types"
    ]
  }
}
